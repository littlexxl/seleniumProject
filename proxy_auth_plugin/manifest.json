
{
  "name": "Proxy Auth Extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [
    "proxy",
    "storage",
    "webRequest",
    "webRequestAuthProvider",
    "webRequestBlocking"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
